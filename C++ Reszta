#include <iostream>

using namespace std;

int main()
{
    int nominal[15] = { 50000, 20000, 10000, 5000, 2000, 1000, 500, 200, 100, 50, 20, 10, 5, 2, 1 };
    double numer;
    cout << "Podaj kwotÄ™ do wydania: ";
    cin >> numer;
    numer = numer * 100;
    int numer2 = (int)numer;
    while (numer2 != 0)
    {
        for (int i = 0; i < 15; i++) 
        {
            if (numer2 >= nominal[i])
            {
                numer2 = numer2 - nominal[i];
                if (nominal[i] >= 100)
                {
                    cout << nominal[i] / 100 << "zl" << " ";
                }
                else
                {
                    cout << nominal[i] << "gr" << " ";
                }
                i--;
            }
        }
    }
    return 0;
}
